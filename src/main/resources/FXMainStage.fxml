          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Button fx:id="saveTimeButton" onAction="#buttonPressed" layoutX="232.0" layoutY="77.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="67.0" text="Button" />
      <PieChart fx:id="charts" layoutX="361.0" layoutY="59.0" prefHeight="400.0" prefWidth="403.0" title="Time">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </PieChart>
      <DateTimePicker fx:id="s" layoutX="25.0" layoutY="77.0" prefHeight="25.0" prefWidth="190.0" />
